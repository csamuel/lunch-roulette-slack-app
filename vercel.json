{
  "version": 2,
  "builds": [
    {
      "src": "api/lunchr.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/lunchr.ts"
    }
  ],
  "env": {
    "YELP_API_KEY": "@YELP_API_KEY",
    "MONGODB_URI": "@MONGODB_URI",
    "SLACK_VERIFICATION_TOKEN": "@SLACK_VERIFICATION_TOKEN"
  }
}
